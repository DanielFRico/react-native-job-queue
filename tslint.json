{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-native"
  ],
  "rules": {
    "radix": false,
    "no-unused-styles": true,
    "arrow-parens": false,
    "cyclomatic-complexity": [true, 15],
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "member-access": false,
    "interface-name": false,
    "no-any": false,
    "jsx-no-lambda": false,
    "no-var-requires": false
  }
}
